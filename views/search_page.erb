<form method = "post" action = "update">

<%unless search_answer == "Need search term" ||search_answer ==  "isnt in phone book" %>

	<%search_answer.each do |row|%>
	<%=row%>
	<%woo = row.join(",")%>
		<input type = "checkbox" name = "info[]" value = "<%=woo%>"> 
		<%row.each do |item| %>
			<%=item%>
		<%end%>
	<br>
	<%end%>

<%else%>
<%=search_answer%>
<%end%>

<input type = "submit" value = "update">
</form>
